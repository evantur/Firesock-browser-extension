{
    "manifest_version": 2,
    "name": "Firesock",
    "version": "1.0",

    "description": "A Firefox Extension to prevent browser fingerprinting",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["sandbox.js"]
        }
    ],

    "web_accessible_resources": [
        "disguise.js"
    ]
}